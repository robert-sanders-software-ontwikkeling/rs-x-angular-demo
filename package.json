    "@angular/router": "^21.1.2",
    "@angular/animations": "^21.1.2",
    "@rs-x/core": "^0.4.12",
    "@rs-x/state-manager": "^0.4.12",
    "@rs-x/expression-parser": "^0.4.12",
    "@rs-x/angular": "^0.4.12",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular/build": "^21.1.2",
    "@angular/cli": "^21.1.2",
